Navigate();\n  const [email, setEmail] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n  const [confirmPassword, setConfirmPassword] = useState(\"\");\n  const [firstname, setFirstname] = useState(\"\");\n  const [lastname, setLastname] = useState(\"\");\n  const [username, setUsername] = useState(\"\");\n  const [loading, setLoading] = useState(false);\n  const [registerErrors, setRegisterErrors] = useState({});\n  const [loginErrors, setLoginErrors] = useState({});\n\n  useEffect(() => {\n    if (user && navigate) {\n      navigate(\"/dashboard\");\n    }\n  }, [user, navigate]);\n\n  const resetErrors = () => {\n    setRegisterErrors({});\n    setLoginErrors({});\n  };\n\n  const onSubmit = (e) => {\n    e.preventDefault();\n    setLoading(true);\n\n    let data = {};\n\n    if (register) {\n      // Check if password and confirmPassword match\n      if (password !== confirmPassword) {\n        setRegisterErrors({ confirmPassword: \"Passwords do not match\" });\n        setLoading(false);\n        return;\n      }\n      data = {\n        firstname,\n        lastname,\n        username,\n        email,\n        password,\n        confirmPassword,\n      };\n    } else {\n      data = {\n        email,\n        password,\n      };\n    }\n\n    register &&\n      axios\n        .post(\"api/auth/register\", data)\n        .then((response) => {\n          const token = response.data.token;\n          localStorage.setItem(\"token\", token);\n          getCurrentUser();\n        })\n        .catch((err) => {\n          setLoading(false);\n          if (err?.response?.data) {\n            setRegisterErrors({ register: err.response.data });\n          }\n        });\n\n    !register &&\n      axios\n        .post(\"api/auth/authenticate\", data)\n        .then((response) => {\n          const token = response.data.token;\n          localStorage.setItem(\"token\", token);\n          getCurrentUser();\n        })\n        .catch((err) => {\n          setLoading(false);\n          if (err?.response?.data) {\n            setLoginErrors({login : err.response.data});\n          }\n        });\n  };\n\n  return (\n    <div className=\"auth\">\n      <div className=\"auth__box\">\n        <div className=\"auth__header\">\n          <h1>{register ? \"Register\" : \"Login\"}</h1>\n        </div>\n        <form onSubmit={onSubmit}>\n          {register && (\n            <div className=\"auth__field\">\n              <label>First Name</label>\n              <input\n                type=\"text\"\n                value={firstname}\n                onChange={(e) => setFirstname(e.target.value)}\n              />\n            </div>\n          )}\n          {register && (\n            <div className=\"auth__field\">\n              <label>Last Name</label>\n              <input\n                type=\"text\"\n                value={lastname}\n                onChange={(e) => setLastname(e.target.value)}\n              />\n            </div>\n          )}\n          {register && (\n            <div className=\"auth__field\">\n              <label>Username</label>\n              <input\n                type=\"text\"\n                value={username}\n                onChange={(e) => setUsername(e.target.value)}\n              />\n            </div>\n          )}\n\n          <div className=\"auth__field\">\n            <label>Email</label>\n            <input\n              type=\"text\"\n              value={email}\n              onChange={(e) => setEmail(e.target.value)}\n            />\n          </div>\n\n          <div className=\"auth__field\">\n            <label>Password</label>\n            <input\n              type=\"password\"\n              value={password}\n              onChange={(e) => setPassword(e.target.value)}\n            />\n          </div>\n\n          {register && (\n            <div className=\"auth__field\">\n              <label>Confirm Password</label>\n              <input\n                type=\"password\"\n                value={confirmPassword}\n                onChange={(e) => setConfirmPassword(e.target.value)}\n              />\n              {registerErrors.confirmPassword && (\n                <p className=\"auth__error\">{registerErrors.confirmPassword}</p>\n              )}\n            </div>\n          )}\n          <div className=\"auth__footer\">\n            {registerErrors.register && (\n              <p className=\"auth__error\">{registerErrors.register}</p>\n            )}\n\n            {loginErrors.login && (\n              <p className=\"auth__error\">{loginErrors.login}</p>\n            )}\n\n            <button className=\"btn\" type=\"submit\">\n              {register ? \"Register\" : \"Login\"}\n            </button>\n            {!register ? (\n              <div className=\"auth__register\">\n                <p>\n                  Not a member? <Link to=\"/register\" onClick={resetErrors}>Register now</Link>\n                </p>\n              </div>\n            ) : (\n              <div className=\"auth__register\">\n                <p>\n                  Already a member? <Link to=\"/\" onClick={resetErrors}>Login now</Link>\n                </p>\n              </div>\n            )}\n          </div>\n        </form>\n      </div>\n    </div>\n  );\n}\n\nexport default AuthBox;\n","/Users/andychen/Projects/Todo-List-Application-Frontend/src/components/Header.js",[],[],"/Users/andychen/Projects/Todo-List-Application-Frontend/src/components/Dashboard.js",[],[],"/Users/andychen/Projects/Todo-List-Application-Frontend/src/components/NewToDo.js",[],[],"/Users/andychen/Projects/Todo-List-Application-Frontend/src/components/ToDoCard.js",[],[],{"ruleId":"54","severity":1,"message":"55","line":15,"column":10,"nodeType":"56","messageId":"57","endLine":15,"endColumn":17},"no-unused-vars","'loading' is assigned a value but never used.","Identifier","unusedVar"]